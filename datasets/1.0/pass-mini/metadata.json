  "@type": "sc:Dataset",
  "name": "Mini-PASS",
  "description": "Smaller downloadable version of PASS to be used in unit tests.",
  "conformsTo": "http://mlcommons.org/croissant/1.0",
  "citeAs": "None",
  "license": "None",
  "url": "None",
  "distribution": [
    {
      "@type": "cr:FileObject",
      "@id": "metadata",
      "name": "metadata",
      "contentUrl": "data/pass_metadata.csv",
      "encodingFormat": "text/csv",
      "sha256": "22f083fb90d4d6d44c5492481c99049a960cf1dcbae71020bd87b94cb4141dae"
    },
    {
      "@type": "cr:FileObject",
      "@id": "pass0",
      "name": "pass0",
      "contentUrl": "data/PASS.0.tar",
      "encodingFormat": "application/x-tar",
      "sha256": "7e0ae9fd44a42e2b2f647ded8073b380239c2505862fd7eb1eff17430c52a60e"
    },
    {
      "@type": "cr:FileObject",
      "@id": "pass1",
      "name": "pass1",
      "contentUrl": "data/PASS.1.tar",
      "encodingFormat": "application/x-tar",
      "sha256": "a7dfd02caa9eba288e3d60083d2fdeb41c6f8aca771362059b1999b9fdf7ffb2"
    },
    {
      "@type": "cr:FileSet",
      "@id": "image-files",
      "name": "image-files",
      "containedIn": [
        {
          "@id": "pass0"
        },
        {
          "@id": "pass1"
        }
      ],
      "encodingFormat": "image/jpeg",
      "includes": "*.jpg"
    }
  ],
  "recordSet": [
    {
      "@type": "cr:RecordSet",
      "@id": "images",
      "name": "images",
      "key": "hash",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "images/hash",
          "name": "hash",
          "description": "The hash of the image, as computed from YFCC-100M.",
          "dataType": "sc:Text",
          "references": {
            "fileObject": {
              "@id": "metadata"
            },
            "extract": {
              "column": "hash"
            }
          },
          "source": {
            "fileSet": {
              "@id": "image-files"
            },
            "extract": {
              "fileProperty": "filename"
            },
            "transform": {
              "regex": "([^\\/]*)\\.jpg"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "images/image_content",
          "name": "image_content",
          "description": "The content of the image.",
          "dataType": "sc:ImageObject",
          "source": {
            "fileSet": {
              "@id": "image-files"
            },
            "extract": {
              "fileProperty": "content"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "images/creator_uname",
          "name": "creator_uname",
          "description": "Unique name of photo creator.",
          "dataType": "sc:Text",
          "source": {
            "fileObject": {
              "@id": "metadata"
            },
            "extract": {
              "column": "unickname"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "images/gps_coordinates",
          "name": "gps_coordinates",
          "description": "GPS coordinates where the image was taken.",
          "dataType": "sc:GeoCoordinates",
          "subField": [
            {
              "@type": "cr:Field",
              "@id": "images/latitude",
              "name": "latitude",
              "dataType": "sc:Float",
              "source": {
                "fileObject": {
                  "@id": "metadata"
                },
                "extract": {
                  "column": "latitude"
                }
              }
            },
            {
              "@type": "cr:Field",
              "@id": "images/longitude",
              "name": "longitude",
              "dataType": "sc:Float",
              "source": {
                "fileObject": {
                  "@id": "metadata"
                },
                "extract": {
                  "column": "longitude"
                }
              }
            }
          ]
        },
        {
          "@type": "cr:Field",
          "@id": "images/date_taken",
          "name": "date_taken",
          "description": "The date the photo was taken.",
          "dataType": "sc:Date",
          "source": {
            "fileObject": {
              "@id": "metadata"
            },
            "extract": {
              "column": "datetaken"
            },
            "transform": {
              "format": "%Y-%m-%d %H:%M:%S.%f"
            }
          }
        }
      ]
    }
  ]
}
