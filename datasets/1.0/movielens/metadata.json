  "@type": "sc:Dataset",
  "name": "Movielens-25M",
  "conformsTo": "http://mlcommons.org/croissant/1.0",
  "description": "MovieLens 25M movie ratings. Stable benchmark dataset. 25 million ratings and one million tag applications applied to 62,000 movies by 162,000 users. Includes tag genome data with 15 million relevance scores across 1,129 tags. Released 12/2019",
  "url": "https://grouplens.org/datasets/movielens/25m/",
  "distribution": [
    {
      "@type": "sc:FileObject",
      "name": "ml-25m-archive",
      "contentUrl": "https://files.grouplens.org/datasets/movielens/ml-25m.zip",
      "encodingFormat": "application/zip",
      "sha256": "6b51fb2759a8657d3bfcbfc42b592ada"
    },
    {
      "@type": "sc:FileObject",
      "name": "ratings-table",
      "contentUrl": "ratings.csv",
      "containedIn": "ml-25m-archive",
      "encodingFormat": "text/csv"
    },
    {
      "@type": "sc:FileObject",
      "name": "movies-table",
      "contentUrl": "movies.csv",
      "containedIn": "ml-25m-archive",
      "encodingFormat": "text/csv"
    },
    {
      "@type": "sc:FileObject",
      "name": "tags-table",
      "contentUrl": "tags.csv",
      "containedIn": "ml-25m-archive",
      "encodingFormat": "text/csv"
    },
    {
      "@type": "sc:FileObject",
      "name": "links-table",
      "contentUrl": "links.csv",
      "containedIn": "ml-25m-archive",
      "encodingFormat": "text/csv"
    },
    {
      "@type": "sc:FileObject",
      "name": "genome-scores-table",
      "contentUrl": "genome-scores.csv",
      "containedIn": "ml-25m-archive",
      "encodingFormat": "text/csv"
    },
    {
      "@type": "sc:FileObject",
      "name": "genome-tags-table",
      "contentUrl": "genome-tags.csv",
      "containedIn": "ml-25m-archive",
      "encodingFormat": "text/csv"
    }
  ],
  "recordSet": [
    {
      "@type": "cr:RecordSet",
      "name": "movies",
      "key": "movie_id",
      "field": [
        {
          "@type": "cr:Field",
          "name": "movie_id",
          "dataType": "sc:Integer",
          "source": {
            "fileObject": "movies-table",
            "extract": {
              "column": "movieId"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "title",
          "description": "The title of the  movie with the release year in parentheses.",
          "dataType": "sc:Text",
          "source": {
            "fileObject": "movies-table",
            "extract": {
              "column": "title"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "genre",
          "description": "A sequence of genres to which the rated movie belongs.",
          "dataType": "sc:Text",
          "repeated": "true",
          "source": {
            "fileObject": "movies-table",
            "extract": {
              "column": "genres"
            },
            "transform": {
              "separator": "|"
            }
          }
        }
      ]
    },
    {
      "@type": "cr:RecordSet",
      "name": "ratings",
      "key": [
        "user_id",
        "movie_id"
      ],
      "field": [
        {
          "@type": "cr:Field",
          "name": "user_id",
          "dataType": "sc:Integer",
          "source": {
            "fileObject": "ratings-table",
            "extract": {
              "column": "userId"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "movie_id",
          "dataType": "sc:Integer",
          "references": {
            "field": "movies/movie_id"
          },
          "source": {
            "fileObject": "ratings-table",
            "extract": {
              "column": "movieId"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "rating",
          "description": "The score of the rating on a five-star scale.",
          "dataType": "sc:Float",
          "source": {
            "fileObject": "ratings-table",
            "extract": {
              "column": "rating"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "timestamp",
          "dataType": "sc:Integer",
          "source": {
            "fileObject": "ratings-table",
            "extract": {
              "column": "timestamp"
            }
          }
        }
      ]
    },
    {
      "@type": "cr:RecordSet",
      "name": "tags",
      "key": [
        "user_id",
        "movie_id",
        "timestamp"
      ],
      "field": [
        {
          "@type": "cr:Field",
          "name": "user_id",
          "dataType": "sc:Integer",
          "source": {
            "fileObject": "tags-table",
            "extract": {
              "column": "userId"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "movie_id",
          "dataType": "sc:Integer",
          "references": {
            "field": "movies/movie_id"
          },
          "source": {
            "fileObject": "tags-table",
            "extract": {
              "column": "movieId"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "tag",
          "description": "Tags assigned to the movie.",
          "dataType": "sc:Text",
          "source": {
            "fileObject": "tags-table",
            "extract": {
              "column": "tag"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "timestamp",
          "dataType": "sc:Integer",
          "source": {
            "fileObject": "tags-table",
            "extract": {
              "column": "timestamp"
            }
          }
        }
      ]
    },
    {
      "@type": "cr:RecordSet",
      "name": "movies_with_ratings_with_tags",
      "key": "movie_id",
      "field": [
        {
          "@type": "cr:Field",
          "name": "movie_id",
          "source": {
            "field": "movies/movie_id"
          }
        },
        {
          "@type": "cr:Field",
          "name": "movie_title",
          "source": {
            "field": "movies/title"
          }
        },
        {
          "@type": "cr:Field",
          "name": "movie_genres",
          "source": {
            "field": "movies/genre"
          }
        },
        {
          "@type": "cr:Field",
          "name": "ratings",
          "dataType": "cr:RecordSet",
          "parentField": {
            "references": {
              "field": "movies/movie_id"
            },
            "source": {
              "field": "ratings/movie_id"
            }
          },
          "subField": [
            {
              "@type": "cr:Field",
              "name": "user_id",
              "source": {
                "field": "ratings/user_id"
              }
            },
            {
              "@type": "cr:Field",
              "name": "rating",
              "source": {
                "field": "ratings/rating"
              }
            },
            {
              "@type": "cr:Field",
              "name": "timestamp",
              "source": {
                "field": "ratings/timestamp"
              }
            }
          ]
        },
        {
          "@type": "cr:Field",
          "name": "tags",
          "dataType": "cr:RecordSet",
          "parentField": {
            "references": {
              "field": "movies/movie_id"
            },
            "source": {
              "field": "tags/movie_id"
            }
          },
          "subField": [
            {
              "@type": "cr:Field",
              "name": "user_id",
              "source": {
                "field": "tags/user_id"
              }
            },
            {
              "@type": "cr:Field",
              "name": "tag",
              "source": {
                "field": "tags/tag"
              }
            },
            {
              "@type": "cr:Field",
              "name": "timestamp",
              "source": {
                "field": "tags/timestamp"
              }
            }
          ]
        }
      ]
    }
  ]
}
