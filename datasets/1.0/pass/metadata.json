  "recordSet": [
    {
      "@type": "cr:RecordSet",
      "name": "images",
      "key": "hash",
      "field": [
        {
          "@type": "cr:Field",
          "name": "hash",
          "description": "The hash of the image, as computed from YFCC-100M.",
          "dataType": "sc:Text",
          "references": {
            "fileObject": "metadata",
            "extract": {
              "column": "hash"
            }
          },
          "source": {
            "fileSet": "image-files",
            "extract": {
              "fileProperty": "filename"
            },
            "transform": {
              "regex": "([^\\/]*)\\.jpg"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "image_content",
          "description": "The content of the image.",
          "dataType": "sc:ImageObject",
          "source": {
            "fileSet": "image-files",
            "extract": {
              "fileProperty": "content"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "creator_uname",
          "description": "Unique name of photo creator.",
          "dataType": "sc:Text",
          "source": {
            "fileObject": "metadata",
            "extract": {
              "column": "unickname"
            }
          }
        },
        {
          "@type": "cr:Field",
          "name": "gps_coordinates",
          "description": "GPS coordinates where the image was taken.",
          "dataType": "sc:GeoCoordinates",
          "subField": [
            {
              "@type": "cr:Field",
              "name": "latitude",
              "dataType": "sc:Float",
              "source": {
                "fileObject": "metadata",
                "extract": {
                  "column": "latitude"
                }
              }
            },
            {
              "@type": "cr:Field",
              "name": "longitude",
              "dataType": "sc:Float",
              "source": {
                "fileObject": "metadata",
                "extract": {
                  "column": "longitude"
                }
              }
            }
          ]
        },
        {
          "@type": "cr:Field",
          "name": "date_taken",
          "description": "The date the photo was taken.",
          "dataType": "sc:Date",
          "source": {
            "fileObject": "metadata",
            "extract": {
              "column": "datetaken"
            },
            "transform": {
              "format": "%Y-%m-%d %H:%M:%S.%f"
            }
          }
        }
      ]
    }
  ]
