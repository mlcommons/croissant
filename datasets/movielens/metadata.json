{
  "@context": {
    "@vocab": "https://schema.org/",
    "applyTransform": "ml:applyTransform",
    "data": "ml:data",
    "dataType": "ml:dataType",
    "field": "ml:Field",
    "format": "ml:format",
    "includes": "ml:includes",
    "ml": "http://mlcommons.org/schema/",
    "recordSet": "ml:RecordSet",
    "references": "ml:references",
    "regex": "ml:regex",
    "replace": "ml:replace",
    "sc": "https://schema.org/",
    "separator": "ml:separator",
    "source": "ml:source",
    "subField": "ml:SubField",
    "wd": "https://www.wikidata.org/wiki/"
  },
  "@type": "sc:Dataset",
  "name": "Movielens-25M",
  "description": "MovieLens 25M movie ratings. Stable benchmark dataset. 25 million ratings and one million tag applications applied to 62,000 movies by 162,000 users. Includes tag genome data with 15 million relevance scores across 1,129 tags. Released 12/2019",
  "@language": "en",
  "url": "https://grouplens.org/datasets/movielens/25m/",
  "distribution": [
    {
      "@type": "sc:FileObject",
      "name": "ml-25m-archive",
      "contentUrl": "https://files.grouplens.org/datasets/movielens/ml-25m.zip",
      "encodingFormat": "application/zip",
      "sha256": "6b51fb2759a8657d3bfcbfc42b592ada"
    },
    {
      "@type": "sc:FileObject",
      "name": "ratings-table",
      "containedIn": "#{ml-25m-archive}",
      "contentUrl": "ratings.csv",
      "encodingFormat": "text/csv"
    },
    {
      "@type": "sc:FileObject",
      "name": "movies-table",
      "containedIn": "#{ml-25m-archive}",
      "contentUrl": "movies.csv",
      "encodingFormat": "text/csv"
    },
    {
      "@type": "sc:FileObject",
      "name": "tags-table",
      "containedIn": "#{ml-25m-archive}",
      "contentUrl": "tags.csv",
      "encodingFormat": "text/csv"
    },
    {
      "@type": "sc:FileObject",
      "name": "links-table",
      "containedIn": "#{ml-25m-archive}",
      "contentUrl": "links.csv",
      "encodingFormat": "text/csv"
    },
    {
      "@type": "sc:FileObject",
      "name": "genome-scores-table",
      "containedIn": "#{ml-25m-archive}",
      "contentUrl": "genome-scores.csv",
      "encodingFormat": "text/csv"
    },
    {
      "@type": "sc:FileObject",
      "name": "genome-tags-table",
      "containedIn": "#{ml-25m-archive}",
      "contentUrl": "genome-tags.csv",
      "encodingFormat": "text/csv"
    }
  ],
  "recordSet": [
    {
      "@type": "ml:RecordSet",
      "name": "movies",
      "key": "#{movie_id}",
      "source": "#{movies-table}",
      "field": [
        {
          "@type": "ml:Field",
          "name": "movie_id",
          "dataType": "sc:Integer",
          "source": "#{movies-table/movieId}"
        },
        {
          "@type": "ml:Field",
          "name": "title",
          "description": "The title of the  movie with the release year in parentheses.",
          "dataType": "sc:Text",
          "source": "#{movies-table/title}"
        },
        {
          "@type": "ml:Field",
          "name": "genre",
          "description": "A sequence of genres to which the rated movie belongs.",
          "dataType": "sc:Text",
          "repeated": "true",
          "source": {
            "applyTransform": {
              "separator": "|"
            },
            "data": "#{movies-table/genres}"
          }
        }
      ]
    },
    {
      "@type": "ml:RecordSet",
      "name": "ratings",
      "key": [
        "#{user_id}",
        "#{movie_id}"
      ],
      "source": "#{ratings-table}",
      "field": [
        {
          "@type": "ml:Field",
          "name": "user_id",
          "dataType": "sc:Integer",
          "source": "#{ratings-table/userId}"
        },
        {
          "@type": "ml:Field",
          "name": "movie_id",
          "dataType": "sc:Integer",
          "references": "#{movies}",
          "source": "#{ratings-table/movieId}"
        },
        {
          "@type": "ml:Field",
          "name": "rating",
          "description": "The score of the rating on a five-star scale.",
          "dataType": "sc:Float",
          "source": "#{ratings-table/rating}"
        },
        {
          "@type": "ml:Field",
          "name": "timestamp",
          "dataType": "sc:Integer",
          "source": "#{ratings-table/timestamp}"
        }
      ]
    },
    {
      "@type": "ml:RecordSet",
      "name": "tags",
      "key": [
        "#{user_id}",
        "#{movie_id}",
        "#{timestamp}"
      ],
      "source": "#{tags-table}",
      "field": [
        {
          "@type": "ml:Field",
          "name": "user_id",
          "dataType": "sc:Integer",
          "source": "#{tags-table/userId}"
        },
        {
          "@type": "ml:Field",
          "name": "movie_id",
          "dataType": "sc:Integer",
          "references": "#{movies}",
          "source": "#{tags-table/movieId}"
        },
        {
          "@type": "ml:Field",
          "name": "tag",
          "description": "Tags assigned to the movie.",
          "dataType": "sc:Text",
          "source": "#{tags-table/tag}"
        },
        {
          "@type": "ml:Field",
          "name": "timestamp",
          "dataType": "sc:Integer",
          "source": "#{tags-table/timestamp}"
        }
      ]
    },
    {
      "@type": "ml:RecordSet",
      "name": "movies_with_ratings_with_tags",
      "key": "#{movie_id}",
      "source": "#{movies}",
      "field": [
        {
          "@type": "ml:Field",
          "name": "movie_id",
          "source": "#{movies/movie_id}"
        },
        {
          "@type": "ml:Field",
          "name": "movie_title",
          "source": "#{movies/title}"
        },
        {
          "@type": "ml:Field",
          "name": "movie_genres",
          "source": "#{movies/genre}"
        },
        {
          "@type": "ml:Field",
          "name": "ratings",
          "dataType": "ml:RecordSet",
          "parentField": {
            "references": "#{movies}",
            "source": "#{ratings/movie_id}"
          },
          "source": "#{ratings}",
          "subField": [
            {
              "@type": "ml:Field",
              "name": "user_id",
              "source": "#{ratings/user_id}"
            },
            {
              "@type": "ml:Field",
              "name": "rating",
              "source": "#{ratings/rating}"
            },
            {
              "@type": "ml:Field",
              "name": "timestamp",
              "source": "#{ratings/timestamp}"
            }
          ]
        },
        {
          "@type": "ml:Field",
          "name": "tags",
          "dataType": "ml:RecordSet",
          "parentField": {
            "references": "#{movies}",
            "source": "#{tags/movie_id}"
          },
          "source": "#{tags}",
          "subField": [
            {
              "@type": "ml:Field",
              "name": "user_id",
              "source": "#{tags/user_id}"
            },
            {
              "@type": "ml:Field",
              "name": "tag",
              "source": "#{tags/tag}"
            },
            {
              "@type": "ml:Field",
              "name": "timestamp",
              "source": "#{tags/timestamp}"
            }
          ]
        }
      ]
    }
  ]
}
