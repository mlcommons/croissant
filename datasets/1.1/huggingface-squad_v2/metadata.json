          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-squad_v2"
            },
            "extract": {
              "column": "question"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "squad_v2/answers",
          "subField": [
            {
              "@type": "cr:Field",
              "@id": "squad_v2/answers/text",
              "dataType": "sc:Text",
              "source": {
                "fileSet": {
                  "@id": "parquet-files-for-config-squad_v2"
