              "source": {
                "fileSet": {
                  "@id": "parquet-files-for-config-squad_v2"
                },
                "extract": {
                  "column": "answers"
                },
                "transform": {
                  "jsonPath": "answer_start"
                }
              },
              "isArray": true,
              "arrayShape": "-1"
            }
          ]
        }
      ]
    }
