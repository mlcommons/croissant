{
    "@context": {
        "@vocab": "https://schema.org/",
        "sc": "https://schema.org/",
        "ml": "http://mlcommons.org/schema/",
        "recordSet": "ml:RecordSet",
        "field": "ml:Field",
        "dataType": "ml:dataType",
        "source": "ml:source"
    },
    "@type": "sc:Dataset",
    "@language": "en",
    "name": "simple-split",
    "description": "An artificial example dataset defining splits from a CSV column",
    "url": "https://mlcommons.org",
    "license": "https://creativecommons.org/licenses/by/4.0/",
    "distribution": [
        {
            "name": "books.csv",
            "@type": "sc:FileObject",
            "contentUrl": "data/books.csv",
            "sha256": "",
            "encodingFormat": "text/csv"
        }
    ],
    "recordSet": [
        {
            "name": "books",
            "@type": "ml:RecordSet",
            "field": [
                {
                    "name": "title",
                    "@type": "ml:Field",
                    "description": "The title of the book",
                    "dataType": "sc:Text",
                    "source": "#{books.csv/Title}"
                },
                {
                    "name": "Author",
                    "@type": "ml:Field",
                    "description": "The author of the book",
                    "dataType": "sc:Text",
                    "source": "#{books.csv/Author}"
                },
                {
                    "name": "split",
                    "@type": "ml:Field",
                    "description": "The data split to which belongs the record",
                    "dataType": [
                        "sc:Text",
                        "wd:Q3985153"
                    ],
                    "source": "#{books.csv/Split}"
                }
            ]
        }
    ]
}
